{
  "name": "btb2",
  "description": "",
  "manifest_version": 2,
  "version": "1.0.0",
  "icons": {
    
  },
  "browser_action": {
      "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": [
        "content.js"
      ]
    }
  ]
}
