{
  "name": "btb2",
  "description": "",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "service_worker": {
    "script": "background.js",
    "background": {
      "service_worker": "background.js"
    }
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "js": [
        "content.js"
      ]
    }
  ]
}